interface IBuffer
{
    ISequence empty();
    int size();
}

interface IChannel
{
    <ISequence, ISequence> read(ISequence data);
    <ISequence, ISequence> write(ISequence data);
}

interface ISequence: IChannel
{
    IBuffer buffer();

    ISequence take(int n);
    ISequence drop(int n);
    ISequence expand(int size);

    int size();
}

class BufferSequence: ISequence
{
   
}
