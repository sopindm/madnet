(sequence [1000 :circular [:element :byte] :direct] [0 0] [0 1000])

(stack proxy1
       proxy2
       ...
       proxyN
       true-seq)

(push stack (bomb 0.1))

(def http-reader
  (chain
    (reader *my-tcp-socket*) :: Nothing -> bytes
    (byte->char 1024) :: bytes->chars
    (char->line 1024) :: chars->strings
    (http-parser) :: strings->http-requests))
  
